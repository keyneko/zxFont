{
  "name": "zxfont",
  "version": "1.0.0",
  "description": "Easy to use bitmap instead of art font in web page",
  "main": "webpack.config.js",
  "scripts": {
    "build": "cp ./src/*.html ./build/ & NODE_ENV=production webpack -p --progress --config webpack.config.js",
    "dev": "cp ./src/*.html ./build/ & webpack-dev-server --devtool eval --hot",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keyneko/zxFont.git"
  },
  "keywords": [
    "bitmap",
    "font"
  ],
  "author": "keyneko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keyneko/zxFont/issues"
  },
  "homepage": "https://github.com/keyneko/zxFont",
  "dependencies": {
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0",
    "webpack-md5-hash": "0.0.4"
  }
}
